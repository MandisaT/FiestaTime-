<div  class="inviteshow">
	<div class="lobster">Create an Item </div>
<%= form_for @product do |f| %>
	<%=f.label "name" %>
	<br>
	<%=f.text_field :name %>
	<br>
	<%=f.label "price" %>
	<br>
	<%=f.text_field :price %>
	
	<br>
	<%=f.label "quantity" %>
	<br>
	<%=f.text_field :quantity %>
		<br>
	<%=f.submit%>
	<br>
<%end%>
</div>